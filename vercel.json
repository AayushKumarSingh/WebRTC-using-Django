{
    "builds": [{
        "src": "djangoProject/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "pipFile": "Pipfile"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "djangoProject/wsgi.py"
        }
    ]
}
